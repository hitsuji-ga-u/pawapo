          <button id="PasteShpaeDistancesY" label="paste y distance" showLabel="false" imageMso="CreateDiagram" onAction="PasteShpaeDistancesY" keytip="Ded"/>

          <button id="MoveToAnchor" label="move to anchor" showLabel="false" imageMso="ControlMarginsGallery" onAction="MoveToAnchor" keytip="Dds"/>
          <button id="DrowExpension" label="draw expansion" showLabel="false" image="Gradient" onAction="PaintGradation" keytip="Ddf"/>

          <button id="TableWidthAutoFit" label="auto fit table width" showLabel="false" imageMso="SpeakByRows" onAction="TableWidthAutoFit" keytip="Dde"/>
          <button id="clip_path" label="clip path" showLabel="false" imageMso="HeaderFooterFilePathInsert" onAction="ClipPath" keytip="Ddr"/>
          <button id="DeleteAndPasteShape" label="delete and paste" showLabel="false" imageMso="Paste" onAction="DeleteAndPasteShape" keytip="V"/>
        </group>

        <group id="font">
          <button id="ChangeShapeColorDark1" label="change shape color to Dark1" showLabel="false" image="ShapeColorDark1" onAction="ChangeShapeColorDark1" keytip="Fsa"/>
          <button id="ChangeLineColorDark1" label="change line color to Dark1" showLabel="false" image="ShapeLineColorDark1" onAction="ChangeLineColorDark1" keytip="Fda"/>
          <button id="ChangeTextColorDark1" label="change font color to Dark1" showLabel="false" image="FontColorDark1" onAction="ChangeTextColorDark1" keytip="Ffa"/>

