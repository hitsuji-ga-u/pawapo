          <button id="ChangeLineColorAccent2" label="change line color to Accent2" showLabel="false" image="ShapeLineColorAccent2" onAction="ChangeLineColorAccent2" keytip="Fdf"/>
          <button id="ChangeTextColorAccent2" label="change font color to Accent2" showLabel="false" image="FontColorAccent2" onAction="ChangeTextColorAccent2" keytip="Fff"/>

          <button id="ChangeShapeColorAccent3" label="change shape color to Accent3" showLabel="false" image="ShapeColorAccent3" onAction="ChangeShapeColorAccent3" keytip="Fsz"/>
          <button id="ChangeLineColorAccent3" label="change line color to Accent3" showLabel="false" image="ShapeLineColorAccent3" onAction="ChangeLineColorAccent3" keytip="Fdz"/>
          <button id="ChangeTextColorAccent3" label="change font color to Accent3" showLabel="false" image="FontColorAccent3" onAction="ChangeTextColorAccent3" keytip="Ffz"/>

          <button id="ChangeShapeColorAccent4" label="change shape color to Accent4" showLabel="false" image="ShapeColorAccent4" onAction="ChangeShapeColorAccent4" keytip="Fsx"/>
          <button id="ChangeLineColorAccent4" label="change line color to Accent4" showLabel="false" image="ShapeLineColorAccent4" onAction="ChangeLineColorAccent4" keytip="Fdx"/>
          <button id="ChangeTextColorAccent4" label="change font color to Accent4" showLabel="false" image="FontColorAccent4" onAction="ChangeTextColorAccent4" keytip="Ffx"/>

          <button id="ChangeShapeColorAccent5" label="change shape color to Accent5" showLabel="false" image="ShapeColorAccent5" onAction="ChangeShapeColorAccent5" keytip="Fsc"/>
          <button id="ChangeLineColorAccent5" label="change line color to Accent5" showLabel="false" image="ShapeLineColorAccent5" onAction="ChangeLineColorAccent5" keytip="Fdc"/>
          <button id="ChangeTextColorAccent5" label="change font color to Accent5" showLabel="false" image="FontColorAccent5" onAction="ChangeTextColorAccent5" keytip="Ffc"/>

          <button id="ChangeShapeColorAccent6" label="change shape color to Accent6" showLabel="false" image="ShapeColorAccent6" onAction="ChangeShapeColorAccent6" keytip="Fsv"/>
          <button id="ChangeLineColorAccent6" label="change line color to Accent6" showLabel="false" image="ShapeLineColorAccent6" onAction="ChangeLineColorAccent6" keytip="Fdv"/>
          <button id="ChangeTextColorAccent6" label="change font color to Accent6" showLabel="false" image="FontColorAccent6" onAction="ChangeTextColorAccent6" keytip="Ffv"/>

